{
  "name": "usrv-trueke",
  "version": "1.0.0",
  "description": "usrv-trueke",
  "main": "handler.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test:unit"
    }
  },
  "scripts": {
    "deploy": "serverless deploy -v",
    "lint": "npm run lint:format && npm run lint:duplicate && npm run lint:check",
    "lint:check": "tslint -t stylish --project tsconfig.json -c tslint.json 'src/**/*.ts' && npm run tsc:check",
    "lint:duplicate": "npx jscpd ./src",
    "lint:fix": "tslint -t stylish --fix --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "lint:format": "prettier --write \"./**/*.{ts,js,json,yml,yaml}\" \"./.{pull_review.kushkirc,lintstagedrc,nycrc,prettierrc}\"",
    "package": "serverless package",
    "sonar": "nyc report --reporter=lcov",
    "test:coverage": "npx nyc mocha -config ./.mocharc.nyc.yml src/**/*.spec.ts",
    "test": "npm run test:unit && npm run test:coverage",
    "test:integration": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "npx mocha --watch src/**/*.spec.ts",
    "test:unit": "npx mocha src/**/*.spec.ts",
    "tsc:check": "tsc --project tsconfig.check.json --noEmit",
    "tsc:interface": "kushki json2ts",
    "validate": "npm run depcheck && npm list && npm run lint && npm run test",
    "server:offline": " export USRV_STAGE=qa && export AWS_REGION=us-east-1 && export USRV_COMMIT=1234 && export USRV_BRANCH=master && export AWS_PROFILE=default && node node_modules/serverless/bin/serverless.js offline -s $USRV_STAGE",
    "depcheck": "depcheck . --ignores=\"serverless-*,@types/*,@kushki/*,prettier,@sendgrid/helpers,aws-lambda,aws-xray-sdk-core,acorn,tsutils,rollbar,chai,husky,lint-staged,ajv,ts-loader,tslint-config-prettier,infrastructure,constant,middleware,core,gateway,service,error,repository,types,parse-full-name,cache-loader,source-map-loader,thread-loader,es-abstract\""
  },
  "devDependencies": {
    "@kushki/cli": "1.4.1",
    "@kushki/nyc": "1.0.3",
    "@kushki/tsconfig": "1.1.0",
    "@kushki/tslint": "5.2.12",
    "@types/aws-lambda": "8.10.70",
    "@types/chai": "4.2.14",
    "@types/chai-as-promised": "7.1.3",
    "@types/chai-json-schema": "1.4.6",
    "@types/lodash": "4.14.167",
    "@types/mocha": "8.2.0",
    "@types/mockery": "1.4.29",
    "@types/node": "14.14.20",
    "@types/proxyquire": "1.3.28",
    "@types/request-promise": "4.1.47",
    "@types/sinon": "9.0.10",
    "@types/sinon-chai": "3.2.5",
    "@types/underscore": "1.10.24",
    "cache-loader": "4.1.0",
    "chai": "4.2.0",
    "copy-webpack-plugin": "6.1.1",
    "depcheck": "1.3.1",
    "husky": "4.3.7",
    "jscpd": "3.3.23",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "serverless": "3.19.0",
    "serverless-cloudformation-resource-counter": "0.2.0",
    "serverless-dynamodb-autoscaling": "0.6.2",
    "serverless-enable-api-logs": "2.0.0",
    "serverless-esbuild": "1.33.2",
    "serverless-offline": "8.8.0",
    "serverless-prune-plugin": "2.0.1",
    "serverless-tag-cloud-watch-logs": "1.0.0",
    "sinon": "9.2.3",
    "sinon-chai": "3.6.0",
    "source-map-loader": "1.1.0",
    "terser-webpack-plugin": "4.2.2",
    "thread-loader": "3.0.0",
    "ts-loader": "8.0.14",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tsutils": "3.19.0",
    "typescript": "3.9.7",
    "webpack": "4.44.2"
  },
  "author": "Kushki (dev@kushkipagos.com)",
  "license": "MIT",
  "dependencies": {
    "@kushki/core": "11.2.36",
    "acorn": "6.4.1",
    "ajv": "6.12.5",
    "aws-lambda": "1.0.6",
    "inversify": "5.1.1",
    "jsonpath": "1.1.1",
    "middy": "0.36.0",
    "esbuild": "0.14.50",
    "reflect-metadata": "0.1.13",
    "rollbar": "2.22.0",
    "rxjs": "6.6.7",
    "rxjs-spy": "7.5.3",
    "serverless-plugin-canary-deployments": "^0.8.0",
    "source-map-support": "0.5.19"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/kushki/usrv-trueke"
  },
  "bugs": {
    "url": "https://bitbucket.org/kushki/usrv-trueke/src/master/.gitignore"
  },
  "homepage": "https://bitbucket.org/kushki/usrv-trueke/src/master/README.md"
}
